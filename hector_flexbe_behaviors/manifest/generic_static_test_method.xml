<?xml version="1.0" encoding="UTF-8"?>

<behavior name="Generic Static Test Method">

	<executable package_path="hector_flexbe_behaviors.generic_static_test_method_sm" class="GenericStaticTestMethodSM" />
	<tagstring>robocup, rc19</tagstring>
	<author>Gabriel Huettenberger</author>
	<date>Tue Jan 15 2019</date>
	<description>
		Behavior for executing a generic test method on a known map
	</description>


	<!-- Contained Behaviors -->
	<contains name="Explore Mission Robocup" />
	<contains name="Waypoint mission" />

	<!-- Available Parameters -->
	<params>

		<param type="boolean" name="hazmatEnabled" default="False" label="hazmatEnabled" hint="Sets the hazmatEnabled" />

		<param type="boolean" name="traversabilityMap" default="False" label="traversabilityMap" hint="Sets the traversabilityMap" />

		<param type="boolean" name="waypointFollowing" default="False" label="waypointFollowing" hint="Sets the waypointFollowing" />

		<param type="boolean" name="exploration" default="False" label="exploration" hint="Sets the exploration" />

		<param type="boolean" name="roughTerrain" default="False" label="roughTerrain" hint="Sets the roughTerrain" />

		<param type="boolean" name="repeat" default="True" label="repeat" hint="Sets the repeat" />

		<param type="text" name="specialFunctionality" default="" label="specialFunctionality" hint="Sets the specialFunctionality" />

	</params>


</behavior>